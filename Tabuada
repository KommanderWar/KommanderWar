using System;

namespace EstruturaDeRepetição
{
    class Program
    {
        static void Main(string[] args)
        {
            for (int x = 0; x < 10; x++) {
                for (int y = 0; y < 10; y++)
                {
                    Console.WriteLine(x + " * " + y + " = "+ x * y);
                }

            }
            Console.ReadKey();
        }
    }
}
